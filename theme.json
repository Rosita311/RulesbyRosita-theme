{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "color": {
      "defaultPalette": false,
      "palette": [
        { "slug": "text", "name": "Text", "color": "#192324" },
        { "slug": "primary", "name": "Primary", "color": "#d7233e" },
        { "slug": "accent", "name": "Accent", "color": "#f8c9d7" },
        { "slug": "accent-2", "name": "Accent 2", "color": "#ebebeb" },
        { "slug": "footer", "name": "Footer", "color": "#cbeef3" },
        { "slug": "contrast", "name": "Contrast", "color": "#49b3c1" },
        { "slug": "background", "name": "Background", "color": "#f8f4f5" },
        { "slug": "placeholder", "name": "Placeholder", "color": "#6E6E6E" }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "\"Nunito\", sans-serif",
          "slug": "body",
          "name": "Body"
        },
        {
          "fontFamily": "\"Zilla Slab\", serif",
          "slug": "heading",
          "name": "Heading"
        }
      ],
      "fontSizes": [
        { "slug": "small", "size": "0.875rem", "name": "Small" },
        { "slug": "normal", "size": "1rem", "name": "Normal" },
        { "slug": "large", "size": "1.5rem", "name": "Large" },
        { "slug": "x-large", "size": "2rem", "name": "Extra Large" }
      ]
    },
    "spacing": {
      "units": [ "px", "em", "rem", "%" ]
    },
    "layout": {
      "contentSize": "52.5rem",
      "wideSize": "75rem"
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--background)",
      "text": "var(--wp--preset--color--text)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--body)",
      "fontSize": "var(--wp--preset--font-size--normal)"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)"
        }
      }
    }
  }
}